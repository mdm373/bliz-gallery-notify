{
  "name": "bliz-gallery-notify",
  "version": "1.0.0",
  "description": "a lambda to give me a heads up when new gallery items are for sale",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "mkdist": "mkdirp dist",
    "build": "parcel build src/index.ts --target node --bundle-node-modules",
    "archive": "ts-node src/tasks/archive.ts",
    "package": "run-s clean mkdist build archive"
  },

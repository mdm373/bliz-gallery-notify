    "@types/node": "^10.14.12",
    "archiver": "^3.0.0",
    "cross-env": "^5.2.0",
    "fs-extra": "^7.0.1",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.3",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "aws-sdk": "^2.489.0",
    "moment": "^2.24.0"
  }
